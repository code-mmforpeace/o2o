+$(function() {
+    //定义访问后台，获取头条轮播图列表以及商店类别列表
+    var url = '/frontend/mainpageinfolist';
+
+    //访问后台，获取数据
+    $.getJSON(url, function (data) {
+        if (data.success) {
+            //获取后台modelMap中的数据
+            var headLineList = data.headLineList;
+            var swiperHtml = '';
+            //遍历list，拼接轮播图
+            headLineList.map(function (item, index) {
+                swiperHtml += ''
+                            + '<div class="swiper-slide img-wrap">'
+                            +      '<img class="banner-img" src="'+ item.lineImg +'" alt="'+ item.lineName +'">'
+                            + '</div>';
+            });
+            //将轮播图组赋值给前端控件
+            $('.swiper-wrapper').html(swiperHtml);
+            //设定轮播时间间隔为2秒
+            $(".swiper-container").swiper({
+                autoplay: 2000,
+                //用户对轮播图操作的时候，不停止轮播图的播放
+                autoplayDisableOnInteraction: false
+            });
+            //获取后台传送过来的data取出对应的商店列表
+            var shopCategoryList = data.shopCategoryList;
+            var categoryHtml = '';
+            shopCategoryList.map(function (item, index) {
+                categoryHtml += ''
+                             +  '<div class="col-50 shop-classify" data-category='+ item.shopCategoryId +'>'
+                             +      '<div class="word">'
+                             +          '<p class="shop-title">'+ item.shopCategoryName +'</p>'
+                             +          '<p class="shop-desc">'+ item.shopCategoryDesc +'</p>'
+                             +      '</div>'
+                             +      '<div class="shop-classify-img-warp">'
+                             +          '<img class="shop-img" src="'+ item.shopCategoryImg +'">'
+                             +      '</div>'
+                             +  '</div>';
+            });
+            //传递给前端进行展示
+            $('.row').html(categoryHtml);
+        }
+    });
+
+    //点击我的显示侧栏
+    $('#me').click(function () {
+        $.openPanel('#panel-left-demo');
+    });
+
+    $('.row').on('click', '.shop-classify', function (e) {
+        var shopCategoryId = e.currentTarget.dataset.category;
+        var newUrl = '/Page/ListPage?parentId=' + shopCategoryId;
+        window.location.href = newUrl;
+    });
+
+});
